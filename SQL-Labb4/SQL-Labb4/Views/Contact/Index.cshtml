        <th>
            @Html.DisplayNameFor(model => model.FadelseAr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ForNamn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EfterNamn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)

            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kon)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.KundId }) |
            @Html.ActionLink("Details", "Details", new { id=item.KundId }) |
